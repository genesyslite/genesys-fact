{
    "name": "genesyslite/genesys-fact",
    "description": "Api facturador",
    "type": "library",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mc Giver Avila Portocarrero",
            "email": "xmcgiver12@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "genesyslite/genesys-business": "1.x-dev",
        "genesyslite/genesys-catalog": "v1.x-dev",
        "genesyslite/genesys-crm": "v1.x-dev",
        "genesyslite/genesys-inventory": "v1.x-dev",
        "greenter/ws": "^4.2",
        "guzzlehttp/guzzle": "^7.2",
        "imangazaliev/didom": "^1.16",
        "mpdf/mpdf": "^8.0",
        "mpdf/qrcode": "^1.1",
        "robrichards/xmlseclibs": "^3.1",
        "thiagoalessio/tesseract_ocr": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "GenesysLite\\GenesysFact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GenesysLite\\GenesysFact\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GenesysLite\\GenesysFact\\Providers\\GenesysFactServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../genesys-fact",
            "options": {
                "symlink" : true
            }
        },
        {
            "type": "path",
            "url": "../genesys-ubigee",
            "options": {
                "symlink" : true
            }
        },
        {
            "type": "path",
            "url": "../genesys-catalog",
            "options": {
                "symlink" : true
            }
        },
        {
            "type": "path",
            "url": "../genesys-business",
            "options": {
                "symlink" : true
            }
        },
        {
            "type": "path",
            "url": "../genesys-crm",
            "options": {
                "symlink" : true
            }
        },
        {
            "type": "path",
            "url": "../genesys-inventory",
            "options": {
                "symlink" : true
            }
        }
    ]
}
